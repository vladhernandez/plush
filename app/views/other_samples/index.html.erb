<% for other_sample in @other_samples %>
  <tr>
    <td><%=h other_sample.name %></td>
    <td><%=h other_sample.mailing_address %></td>
    <td><%=h other_sample.mailing_address_2 %></td>
    <td><%=h other_sample.phone_number %></td>
    <td><%=h other_sample.email_address %></td>
    <td><%=h other_sample.event_date %></td>
    <td><%=h other_sample.sample_name_1 %></td>
    <td><%=h other_sample.sample_name_2 %></td>
    <td><%=h other_sample.sample_name_3 %></td>
    <td><%=h other_sample.text_paper %></td>
    <td><%=h other_sample.text_paper_layer_1 %></td>
    <td><%=h other_sample.text_paper_layer_2 %></td>
    <td><%=h other_sample.text_paper_layer_3 %></td>
    <td><%=h other_sample.text_paper_2 %></td>
    <td><%=h other_sample.text_paper_2_layer_1 %></td>
    <td><%=h other_sample.text_paper_2_layer_2 %></td>
    <td><%=h other_sample.text_paper_2_layer_3 %></td>
    <td><%=h other_sample.text_paper_3 %></td>
    <td><%=h other_sample.text_paper_3_layer_1 %></td>
    <td><%=h other_sample.text_paper_3_layer_2 %></td>
    <td><%=h other_sample.text_paper_3_layer_3 %></td>
    <td><%=h other_sample.font_color_1 %></td>
    <td><%=h other_sample.font_color_2 %></td>
    <td><%=h other_sample.font_color_3 %></td>
    <td><%=h other_sample.ribbon_color_1 %></td>
    <td><%=h other_sample.ribbon_color_2 %></td>
    <td><%=h other_sample.ribbon_color_3 %></td>
    <td><%=h other_sample.special_requests %></td>
    <td><%=h other_sample.samples_quantity %></td>
    <td><%=h other_sample.samples_quantity_cost %></td>
    <td><%=h other_sample.paper_samples_quantity %></td>
    <td><%=h other_sample.paper_samples_quantity_cost %></td>
    <td><%=h other_sample.paper_swatch_deck_quantity %></td>
    <td><%=h other_sample.paper_swatch_deck_quantity_cost %></td>
    <td><%=h other_sample.ribbon_swatch_deck_quantity %></td>
    <td><%=h other_sample.ribbon_swatch_deck_quantity_cost %></td>
    <td><%=h other_sample.subtotal %></td>
    <td><%=h other_sample.gst %></td>
    <td><%=h other_sample.total %></td>
    <td><%= link_to 'Show', other_sample %></td>
    <td><%= link_to 'Edit', edit_other_sample_path(other_sample) %></td>
    <td><%= link_to 'Destroy', other_sample, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

