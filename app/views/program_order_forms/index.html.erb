<h1>Listing program_order_forms</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Mailing address</th>
    <th>Mailing address 2</th>
    <th>Shipping address</th>
    <th>Shipping address 2</th>
    <th>Phone number</th>
    <th>Email address</th>
    <th>Wedding date</th>
    <th>Brides name</th>
    <th>Grooms name</th>
    <th>Design contact number</th>
    <th>Design name</th>
    <th>Quantity</th>
    <th>Ordered sample</th>
    <th>Sample number</th>
    <th>Text paper</th>
    <th>Text paper layer 2</th>
    <th>Text paper layer 3</th>
    <th>Text paper layer 4</th>
    <th>Font color</th>
    <th>Ribbon color</th>
    <th>Special requests</th>
    <th>Program text</th>
  </tr>

<% for program_order_form in @program_order_forms %>
  <tr>
    <td><%=h program_order_form.name %></td>
    <td><%=h program_order_form.mailing_address %></td>
    <td><%=h program_order_form.mailing_address_2 %></td>
    <td><%=h program_order_form.shipping_address %></td>
    <td><%=h program_order_form.shipping_address_2 %></td>
    <td><%=h program_order_form.phone_number %></td>
    <td><%=h program_order_form.email_address %></td>
    <td><%=h program_order_form.wedding_date %></td>
    <td><%=h program_order_form.brides_name %></td>
    <td><%=h program_order_form.grooms_name %></td>
    <td><%=h program_order_form.design_contact_number %></td>
    <td><%=h program_order_form.design_name %></td>
    <td><%=h program_order_form.quantity %></td>
    <td><%=h program_order_form.ordered_sample %></td>
    <td><%=h program_order_form.sample_number %></td>
    <td><%=h program_order_form.text_paper %></td>
    <td><%=h program_order_form.text_paper_layer_2 %></td>
    <td><%=h program_order_form.text_paper_layer_3 %></td>
    <td><%=h program_order_form.text_paper_layer_4 %></td>
    <td><%=h program_order_form.font_color %></td>
    <td><%=h program_order_form.ribbon_color %></td>
    <td><%=h program_order_form.special_requests %></td>
    <td><%=h program_order_form.program_text %></td>
    <td><%= link_to 'Show', program_order_form %></td>
    <td><%= link_to 'Edit', edit_program_order_form_path(program_order_form) %></td>
    <td><%= link_to 'Destroy', program_order_form, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

